{
  "steps": {
    "./node_modules/assemblyscript/bin/asc": [
      "--noAssert",
      "--noDebug",
      "index.ts",
      "-t",
      "module.wat"
    ],
    "sed": [
      "-i",
      "/\\(start $start\\)/d",
      "module.wat"
    ],
    "wat2wasm": [
      "module.wat",
      "-o",
      "module.wasm"
    ]
  },
  "artifact": "module.wasm"
}